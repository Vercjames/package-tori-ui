{
  "name": "trpc-swagger",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "apps/*",
    "examples/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "start": "turbo run dev",
    "launch": "turbo run start",
    "clean": "find . -type d \\( -name '.turbo' -o -name '.next' -o -name 'build' -o -name 'node_modules' \\) -prune -exec rm -rf {} +",
    "build": "turbo run build",
    "startup": "turbo run startup",
    "format": "turbo run format",
    "lint": "turbo run lint"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "tsx": "4.19.4",
    "turbo": "^2.5.3"
  }
}
